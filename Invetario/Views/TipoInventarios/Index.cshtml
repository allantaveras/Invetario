@model IEnumerable<Invetario.Models.TipoInventario>

@{
    ViewBag.Title = "Tipo de Invetario";
}

<h2>Tipo de Inventario</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.descripcion_tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cuenta_contable)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.descripcion_tipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cuenta_contable)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id_tipo_inventario }) |
            @Html.ActionLink("Details", "Details", new { id=item.id_tipo_inventario }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id_tipo_inventario })
        </td>
    </tr>
}

</table>
